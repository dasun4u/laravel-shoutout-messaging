{
    "name": "dasun4u/laravel-shoutout-messaging",
    "description": "shoutout messaging library for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dasun Dissanayake",
            "email": "dpdasun@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "7.4.2"
    },
    "autoload": {
        "psr-4": {
            "Dasun4u\\LaravelShoutoutMessaging\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Dasun4u\\LaravelShoutoutMessaging\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": ".\\vendor\\bin\\phpunit .\\tests\\SampleTest.php"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dasun4u\\LaravelShoutoutMessaging\\ShoutoutServiceProvider"
            ]
        }
    }
}
